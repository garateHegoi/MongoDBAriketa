<hibernate-mapping schema="books">
    <class entity-name="model.Book" name="model.Book" table="books">
        <id column="_id" name="_id">
            <generator class="increment"/>
        </id>
        <property column="Name" generated="never" lazy="false" name="Name"/>
    </class>
</hibernate-mapping>